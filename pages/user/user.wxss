.scan{
  width: 100%;
  height: 252rpx;
  background-color: #23a3ff;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.scan .left {
  display: flex;
  align-items: center;
}
.scan .left image{
  width: 124rpx;
  height: 124rpx;
  border-radius: 50%;
}
.scan .left  text{
  font-size: 24rpx;
  color:#fff;
  margin-left: 10rpx;
}
.scan .right {
  display: flex;
  align-items: center;
}
.scan .right image{
  width: 84rpx;
  height: 84rpx;
}
